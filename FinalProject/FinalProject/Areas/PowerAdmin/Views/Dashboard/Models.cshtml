@model IEnumerable<Model>
@{
    ViewData["Title"] = "Models";
    Layout = "~/Areas/PowerAdmin/Views/Shared/_AdminLayout.cshtml";
}

<div class="col-12 mb-4 d-flex justify-content-center" >
    <a asp-area="PowerAdmin" asp-controller="Dashboard" asp-action="CreateModel" style="color:white; display:block;" class="btn btn-success d-flex align-items-center align-items-center">Model Yarat <i style="color:white;font-size:20px;" class="far fa-plus-square"></i></a>
</div>
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Markası</th>
            <th scope="col">Model Adı</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var proModel in Model)
        {
            <tr>
                <td>@proModel.Mark.Name</td>
                <td>@proModel.Name</td>
            </tr>
        }


    </tbody>
</table>


